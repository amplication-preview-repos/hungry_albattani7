datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Reservation {
  date DateTime?
  id   String               @id @default(cuid())
  role EnumReservationRole?
}

model User {
  id       String @id @default(cuid())
  username String @unique
}

enum EnumReservationRole {
  Chiusura
  Apertura
}
